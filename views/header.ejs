        <%=page_title%>
    </title>

</head><!--/head-->

<body id="home" class="homepage">

    <header id="header">
        <nav id="main-menu" class="navbar navbar-default navbar-fixed-top" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- <a class="navbar-brand" href="<%= baseURL %>"><img src="/images/logo.png" alt="logo"></a> -->
                </div>

                <div class="collapse navbar-collapse navbar-right">
                    <ul class="nav navbar-nav">
                        <li><a href="<%= baseURL %>" title="Home">Home</a></li>
                        <li><a href="<%= baseURL %>quem-somos" title="Quem Somos">Quem Somos</a></li>
                        <li><a href="<%= baseURL %>como-funciona" title="Como Funciona">Como Funciona</a></li>
                        <li><a href="<%= baseURL %>fale-conosco" title="Fale Conosco">Fale Conosco</a></li>
                        <% if (session_site.hasOwnProperty('data_user')) { %>
                            <li class="dropdown">
                                <a href="/login" title="Minha Conta" class="orange dropdown-toggle" id="dropdownMenu1" data-toggle="dropdown">
                                    Minha Conta
                                    <span class="caret"></span>
                                </a>
                                <div class="dropdown-menu dropdown-minha-conta" role="menu">
                                    <% if (session_site.data_user.cd_tipo_usuario == 2) { %>
                                    <span class="bem-vindo">Bem-vindo, <%=session_site.data_user.especifico.nm_apoiador%></span>
                                    <% } else if (session_site.data_user.cd_tipo_usuario == 1) { %>
                                    <span class="bem-vindo">Bem-vindo, <%=session_site.data_user.especifico.nm_causa%></span>
                                    <% } else if (session_site.data_user.cd_tipo_usuario == 3) { %>
                                        <span class="bem-vindo">Bem-vindo, <%=session_site.data_user.especifico.nm_parceiro %></span>
                                    <% } %>
                                    <ul class="opcoes">
                                        <li>
                                            <% if (session_site.data_user.cd_tipo_usuario == 2) { %>
                                                <a href="/apoiadores/editar" title="Editar meus dados"> Editar meus dados
                                                </a>
                                            <% } else if (session_site.data_user.cd_tipo_usuario == 1) { %>
                                                <a href="/causas/editar" title="Editar meus dados"> Editar meus dados
                                                </a>
                                            <% } else if (session_site.data_user.cd_tipo_usuario == 3) { %>
                                                <a href="/parceiros/editar" title="Editar meus dados"> Editar meus dados
                                                </a>
                                            <% } %>
                                        </li>
                                    </ul>
                                    <a class="btn btn-primary" id="logout" href="/logout">Sair</a>
                                </div>
                            </li>
                            <% } else { %>
                            <li><a href="<%= baseURL %>apoiadores/cadastro" title="Apoiar uma causa" class="orange">Apoiar</a></li>
                            <li><a href="<%= baseURL %>causas/cadastro" title="Cadastre sua causa" class="orange">Cadastre sua causa</a></li>
                            <li><a href="<%= baseURL %>parceiros/cadastro" title="Seja um parceiro" class="orange">Seja um parceiro</a></li>
                            <li class="dropdown">
                                <a href="/login" title="Entrar" class="orange dropdown-toggle" id="dropdownMenu1" data-toggle="dropdown">
                                    Entrar
                                    <span class="caret"></span>
                                </a>
                                <div class="dropdown-menu dropdown-login" role="menu">
                                    <form role="form">
                                        <div class="form-group">
                                            <label for="email">
                                             <input type="text" name="email" id="email" placeholder="exemplo@exemplo.com" class="form-control" />
                                            </label>
                                        </div>
                                        <div class="form-group">
                                            <label for="password">
                                            <input type="password" name="password" id="password" placeholder="Senha" class="form-control" />
                                            </label>
                                        </div>
                                    </form>
                                    <button type="button" class="btn btn-primary" id="logar">Logar</button>
                                </div>
                            </li>
                        <% } %>
                    </ul>
                </div>
            </div><!--/.container-->
        </nav><!--/nav-->
    </header><!--/header-->
